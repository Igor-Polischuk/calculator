{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@customTypes/*":  ["ts/interfaces/*"],
      "@models/*":  ["ts/models/*"],
      "@controllers/*":  ["ts/controllers/*"],
      "@views/*":  ["ts/views/*"],
    },
    "outDir": "./dist/",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strict": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node"
  }
}