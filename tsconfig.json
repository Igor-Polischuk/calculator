{
  "compilerOptions": {
    "baseUrl": "./src/ts",
    "paths": {
      "@customTypes/*":  ["interfaces/*"],
      "@components/*":  ["components/*"],
      
    },
    "outDir": "./dist/",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strict": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node"
  }
}